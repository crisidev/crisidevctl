{
    "dependencies": "etcdctl,ip",
    "coreos_update_channel": "beta",
    "etcd_keyfile": "/etc/crisidev/etcd.key",
    "firewall_file": "/etc/crisidev/firewall",
    "kvm_bridge": "br0",
    "etcd_host": "etcd.crisidev.org",
    "nginx_config_file": "/etc/nginx/sites-enabled/crisidev-{}",
    "etcd_dir_coreos_subnet": "/coreos.com/network/subnets",
    "etcd_port": 4001,
    "firewall_template": "/etc/crisidev/firewall.tmpl",
    "kvm_user": "USERNAME",
    "kvm_vg_name": "crisidev",
    "nginx_template": "/etc/crisidev/nginx-{}.conf.tmpl",
    "firewall_file_safe": "/etc/crisidev/firewall.safe",
    "etcd_dir_crisidev": "/crisidev/services",
    "etcd_dir_prometheus": "/crisidev/prometheus",
    "etcd_discovery": "https://discovery.etcd.io/new",
    "prometheus_targets_file": "/tmp/prom.yml",
    "gorb_endpoint": "http://192.168.0.1:4672/service",
    "gorb_unit_file": "gorb.crisidev.org.service",
    "etcd_gorb_status_path": "/crisidev/gorb/status",
    "gorb_default_lb_vip": "",
    "gorb_default_lb_persistent": true,
    "gorb_default_lb_method": "wlc",
    "gorb_default_backend_method": "nat",
    "nat_reserved_ports": {
        "22/tcp": "1022/tcp",
        "80/tcp": "80/tcp",
        "443/tcp": "443/tcp"
    }
}
